    "def array_advance(A):\n",
    "    furthest_reached=0\n",
    "    last_idx=len(A)-1\n",
    "    i=0\n",
    "    while i<=furthest_reached and furthest_reached<last_idx:\n",
    "        furthest_reached=max(furthest_reached,A[i]+i)\n",
    "        i+=1\n",
    "        \n",
    "    return furthest_reached>=last_idx\n",
    "\n",
    "A=[3,3,0,0,1,3,3]\n",
    "array_advance(A)"
   ]
